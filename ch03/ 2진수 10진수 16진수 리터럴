using System;

namespace IntegralTypesExample
{
    class Program
    {
        static void Main(string[] args)
        {
            byte a = 240; //10진수 리터럴
            Console.WriteLine($"byte a: {a}");

            byte b = 0b11110000; //2진수 리터럴
            Console.WriteLine($"byte b: {b}");

            byte c = 0xF0; //16진수 리터럴
            Console.WriteLine($"byte c: {c}");

            uint d = 0x1234_abcd; //16진수 리터럴 
            Console.WriteLine($"uint d: {d}");

        }
    }
}
